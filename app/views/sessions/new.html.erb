<div class="container-fluid">
<%= render  'shared/login_nav'%>

  <h1>Login Page</h1>

  <%= form_with(model: @user, url: sessions_path, local: true, method: :post) do |f| %>

    <!-- Field of sessoin form -->
    <div class= >
      <%= f.label :email %>
      <%= f.text_field :email, placeholder: 'Please enter email id' %>
    </div>
    <div class='col-md-3'>
      <%= f.label :password %>
      <%= f.password_field :password, placeholder: 'Please enter password' %>
    </div><br>
    <div class='col-md-3'> 
      <%= f.submit 'Log In' %>
    </div>
      <%= link_to "sign Up new user?",new_user_path %>
  <% end %>
</div>


